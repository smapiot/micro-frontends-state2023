{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "*",
      "methods": ["POST", "PUT", "DELETE"],
      "rewrite": "/api/render"
    },
    {
      "route": "/",
      "rewrite": "/api/render"
    },
    {
      "route": "/index.html",
      "rewrite": "/api/render"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/render"
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github?post_login_redirect_uri=.referrer",
      "statusCode": 302
    },
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
